--2019.07.11

CREATE SEQUENCE GM_MID_JJONG_SEQ INCREMENT BY 1 START WITH 1;

CREATE TABLE GUESTBOOK_MESSAGE_JJONG (
    MESSAGE_ID NUMBER(6) CONSTRAINT GM_MID_JJONG_PK PRIMARY KEY,
    GUEST_NAME VARCHAR(50) NOT NULL,
    PASSWORD VARCHAR(30) NOT NULL,
    MESSAGE LONG NOT NULL
);

INSERT INTO GUESTBOOK_MESSAGE_JJONG (MESSAGE_ID, GUEST_NAME, PASSWORD, MESSAGE)
VALUES (GM_MID_JJONG_SEQ.nextval, ?, ?, ?);